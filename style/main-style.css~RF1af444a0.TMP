.body-div .title-div{
	font-size:16px;
}
.body-div .title-div h2{
	font-size:25px;
	line-height:30px;
}
}






























.info-div{
	width:80%;
	padding:50px 0px 50px 0px;
	margin:auto;
	text-align:center
}
.info-div .info-img{
	display:inline-block;
	vertical-align:top;
	width:43%;
	text-align:center
}
.info-div .info-img img{
	width:90%;
	border-radius:7px;
}




.info-div .info-text{
	display:inline-block;
	vertical-align:top;
	width:55%;
	text-align:left;
}
.info-div .info-text h2{
	font-size:35px;
	padding:0px;
	margin:0px;
	line-height:45px;
}
.info-div .info-text h2 hr{
    width: 200px;
	border:none;
    border-bottom: 3px solid #A53692;
	margin-bottom:0px;
	float:left;
}

.info-div .info-text .text{
	line-height:25px;
	text-align:justify;
}
.info-div .info-text .text span{
	color:#FE5000;
}
.info-div .info-text .text a{
	color:#06F;
}
.info-div .info-text .text a:hover{
	color:#0CF;
	text-decoration:underline;
}

.info-div .info-text .text li{
	list-style:none;
	padding:10px;
}
.info-div .info-text .text li i{
	color:#FE5000;
	margin-right:10px;
}

.info-div .info-text .text .btn{
	border:none;
	height:50px;
	width:40%;
	min-width:200px;
	background:#A53692;
	color:#FFF;
	font-size:15px;
	cursor:pointer;
    font-family:body_text;
-moz-transition:all 0.2s ease-in 0.2s;
transition:all 0.2s ease-in 0.2s;
-webkit-transition:all 0.2s ease-in 0.2s;
-ms-transition:all 0.2s ease-in 0.2s;
-o-transition:all 0.2s ease-in 0.2s;
}
.info-div .info-text .text .btn:hover{
	background:#FE5000;
}
.info-div .info-text .text #btn{
	background:#FE5000;
}
.info-div .info-text .text #btn:hover{
	background:#A53692;
}



@media all and (max-width:1000px) {
.info-div{
	width:96%;
}
}

@media all and (max-width:750px) {
.info-div .info-img{
	width:100%;
	margin-bottom:20px;
}

.info-div .info-text{
	width:96%;
}
}


@media all and (max-width:450px) {
.info-div .info-img img{
	width:98%;
	border-radius:0px;
}
.info-div .info-text .text .btn{
	width:100%;
	margin-bottom:10px;
}
}
@media all and (max-width:360px) {
.info-div .info-text h2{
	font-size:25px;
	line-height:35px;
}
}
















.pricing-back-div{
	min-height:300px;
	margin:auto;
	text-align:center;
	padding-bottom:20px;
	width:80%;
	
}
.pricing-back-div .pricing-div{
	width:29%;
	min-width:280px;
	display:inline-block;
	vertical-align:top;
	min-height:200px;
	padding:20px;
	background:url(../all-images/images/link-bg.svg) bottom right no-repeat;
	margin-bottom:20px;
}
.pricing-back-div .pricing-div .div{
	text-align:center;
	background:#FFF;	
-moz-transition:all 0.2s ease-in 0.2s;
transition:all 0.2s ease-in 0.2s;
-webkit-transition:all 0.2s ease-in 0.2s;
-ms-transition:all 0.2s ease-in 0.2s;
-o-transition:all 0.2s ease-in 0.2s;
cursor:pointer;
}
.pricing-back-div .pricing-div .div:hover{
		-moz-transform:scale(1.03) translate(1px,0px);
		-webkit-transform:scale(1.03) translate(1px,0px);
	box-shadow:0px 0px 40px 1px rgba(0,0,0,.1);
}
.pricing-back-div .pricing-div .div .pix{
	max-height:200px;
	overflow:hidden
}

.pricing-back-div .pricing-div .div .pix img{
	width:100%;
}

.pricing-back-div .pricing-div .text{
	margin-top:-100px;
	text-align:center;
	height: auto;
	background:#FFF;	
	font-size:13px;
}
.pricing-back-div .pricing-div .text .title{
	width:80%;
	min-height:80px;
	background:rgba(0,0,0,.7);
	color:#FE5000;
	position:relative;
	margin:auto;
	padding:10px 0px 10px 0px;
	border-radius:5px 5px 0px 0px;
}
.pricing-back-div .pricing-div .text .title h3{
	font-size:20px;
	padding:0px;
	margin:0px;
	font-family:"Trebuchet MS", Arial, Helvetica, sans-serif;
}
.pricing-back-div .pricing-div .text .title h3 span{
	font-size:50px;
	color:#FFF;
}

.pricing-back-div .pricing-div .text .desc{
	width:90%;
	margin:10px auto;
	margin-bottom:0px;
	padding-bottom:30px;
	font-size:15px;
}
.pricing-back-div .pricing-div .text .desc a{
	text-decoration:underline;
}
.pricing-back-div .pricing-div .text .desc span{
	color:#FE5000;
}
.pricing-back-div .pricing-div .text .desc .exam-desc{
	padding-bottom:10px;
	border-bottom:#CCC 1px solid;
}

.pricing-back-div .pricing-div .text .desc li{
	padding:10px;
	min-height:18px;
	line-height:18px;
	list-style:none;
	text-align:left;
}
.pricing-back-div .pricing-div .text .desc li i{
	width:13px;
	height:13px;
	background:#A53692;
	border-radius:100%;
	font-size:13px;
	color:#FFF;
}
.pricing-back-div .pricing-div .text .desc .btn{
	border:none;
	height:50px;
	width:100%;
	min-width:200px;
	background:#A53692;
	color:#FFF;
	font-size:15px;
	cursor:pointer;
    font-family:body_text;
	margin-top:20px;
-moz-transition:all 0.2s ease-in 0.2s;
transition:all 0.2s ease-in 0.2s;
-webkit-transition:all 0.2s ease-in 0.2s;
-ms-transition:all 0.2s ease-in 0.2s;
-o-transition:all 0.2s ease-in 0.2s;
}
.pricing-back-div .pricing-div .text .desc .btn:hover{
	background:#FE5000;
}


@media all and (max-width:1290px) {
.pricing-back-div{
	width:90%;
}
}

@media all and (max-width:1150px) {
.pricing-back-div .pricing-div{
	width:40%;
}
}
@media all and (max-width:820px) {
.pricing-back-div .pricing-div{
	width:90%;
	min-width:250px;
}
}
@media all and (max-width:400px) {
.pricing-back-div .pricing-div{
	width: auto;
	padding:10px;
}
.pricing-back-div .pricing-div .text h4{
	font-size:20px;
	line-height:25px;
}
}

























